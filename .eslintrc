{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "eol-last": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "operator-linebreak": "off"
  }
}